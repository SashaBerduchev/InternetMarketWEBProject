@model IEnumerable<InternetMarketWEB.Models.DocForPain>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Organization)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contragent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataLoad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataLoad1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dataload2)
